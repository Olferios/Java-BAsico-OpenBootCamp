package com.primero.Repeticion;

public class EjercicioEjemplo {
    public static void main(String[] args) {
        /**
         * Crear un bucle que permita concatenar textos e impria el resultado final por consola
         * concatenar nombres
         * Los textos pueden venir de un array String
         * String[] nombres={"","","",""};
         */

        String[] nombres={"Amor","foraneo","Bogota","Trancones"};
        for(int i=0;i< nombres.length;i++){
            System.out.println(nombres[i]);
        }
    }
}
